#include <iostream>
using namespace std;
int Find_Repeat (int arr[], int size)
{
for (int i = 0; i < size; i++)
{

for (int j = i+1; j < size; j++)
{
if (arr[i] == arr[j])
{
cout<<arr[j]<<endl;
}
}
}
return 0;
}
int main ()
{
int arr[]={2,3,4,5,6,2};
int size =sizeof(arr)/sizeof(arr [0]);

// Create_arr(arr, size);
  Find_Repeat (arr, size);
    return 0;
}


