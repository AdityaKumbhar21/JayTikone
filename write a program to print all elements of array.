#include <iostream>
using namespace std;

void printarray
(int arr[100]);

int  main()
{
int arr1[5]={10,20,30,40,50};
int arr2[5]={5,15,25,35,45};
printarray(arr1);//passing array to function
printarray(arr2);
}

void printarray(int arr[5])
{
	cout<<"print elements of array"<<endl;
	for (int i=0;i<5;i++)
	{
		cout<<arr[i]<<"\n";
	}
}





output:

print elements of array
10
20
30
40
50
print elements of array
5
15
25
35
45

